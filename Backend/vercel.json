{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/noteRoutes.js",
      "dest": "/noteRoutes.js"
    },
    {
      "src": "/userRoutes.js",
      "dest": "/userRoutes.js"
    },
    {
      "src": "/",
      "dest": "/index.js"
    }
  ]
}
