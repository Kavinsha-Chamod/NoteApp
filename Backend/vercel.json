{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "./routes/noteRoutes",
      "dest": "./routes/noteRoutes"
    },
    {
      "src": "./routes/userRoutes.js",
      "dest": "./routes/userRoutes.js"
    },
    {
      "src": "/",
      "dest": "/index.js"
    }
  ]
}
