{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "./routes/userRoutes.js", "dest": "./routes/userRoutes.js" },
    { "src": "./routes/noteRoutes.js", "dest": "./routes/noteRoutes.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
